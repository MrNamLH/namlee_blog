{
	"name": "namlee_blog",
	"version": "1.0.0",
	"author": "namlee",
	"license": "MIT",
	"private": true,
	"scripts": {
		"precommit": "lint-staged",
		"dev": "next src",
		"devServer": "node server/app.js",
		"debug": "node src/server.js --inspect-brk",
		"build": "next build src",
		"export": "next export -o out src",
		"startWithNext": "next start src",
		"start": "SET NODE_ENV=production & node server/app.js",
		"test:lint:js": "eslint . --ext=js --ext=jsx --fix",
		"test:lint:pretty": "prettier --config .prettierrc --write",
		"test:lint:css": "stylelint --config=.stylelintrc.json"
	},
	"lint-staged": {
		"*.{js,jsx,json}": [
			"prettier --config .prettierrc --write",
			"git add"
		],
		"*.{scss,css}": [
			"stylelint --config=.stylelintrc.json --fix",
			"prettier --config .prettierrc --write",
			"git add"
		]
	},
	"dependencies": {
		"antd": "^3.7.3",
		"apollo-server-express": "^2.0.4",
		"cors": "^2.8.4",
		"express": "^4.16.3",
		"express-graphql": "^0.6.12",
		"graphql": "^0.13.2",
		"graphql-tools": "^3.1.1",
		"mongoose": "^5.2.9",
		"next": "^6.1.1",
		"next-redux-wrapper": "^2.0.0",
		"prop-types": "^15.6.2",
		"react": "^16.4.1",
		"react-dom": "^16.4.1",
		"react-redux": "^5.0.7",
		"redux": "^4.0.0",
		"redux-devtools-extension": "^2.13.5",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.3.0",
		"reqwest": "^2.0.5",
		"xhr2": "^0.1.4"
	},
	"devDependencies": {
		"@zeit/next-css": "^0.2.0",
		"@zeit/next-sass": "^0.2.0",
		"babel-eslint": "^8.2.6",
		"babel-plugin-import": "^1.8.0",
		"babel-plugin-transform-decorators-legacy": "^1.3.5",
		"eslint": "^5.2.0",
		"eslint-config-airbnb": "^17.0.0",
		"eslint-config-prettier": "^2.9.0",
		"eslint-config-react-app": "^2.1.0",
		"eslint-plugin-flowtype": "^2.50.0",
		"eslint-plugin-import": "^2.13.0",
		"eslint-plugin-jsx-a11y": "^6.1.1",
		"eslint-plugin-prettier": "^2.6.2",
		"eslint-plugin-react": "^7.10.0",
		"husky": "^0.14.3",
		"lint-staged": "^7.2.0",
		"next-eslint": "^0.0.4",
		"next-progressbar": "^1.0.0",
		"next-purgecss": "^1.0.1",
		"next-runtime-dotenv": "^1.0.1",
		"node-sass": "^4.9.2",
		"nodemon": "^1.18.3",
		"prettier": "^1.14.0",
		"prettier-eslint": "^8.8.2",
		"styled-jsx-plugin-sass": "^0.2.4",
		"stylelint": "^9.4.0",
		"stylelint-config-standard": "^18.2.0"
	}
}
